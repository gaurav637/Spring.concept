<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

      <bean id = "student1" class = "com.spring.spring_concept.Student">
          <property name = "studentId">
              <value>101</value>
          </property>

          <property name = "studentName">
              <value> Gaurav </value>
          </property>

          <property name = "studentAddress">
              <value> Uttarakhand</value>
          </property>
      </bean>

      <bean id = "emp" class = "com.spring.spring_concept.Employee">
          <property name = "id">
              <value>1</value>
          </property>

          <property name = "name">
              <value>Deepansh</value>
          </property>

          <property name = "city">
              <value>punjab</value>
          </property>

          <property name = "salary">
              <value>2000</value>
          </property>

      </bean>

      <bean id = "customer_id" class = "com.spring.spring_concept.customer">
          <property name = "name" value = "sourav"/>
          <property name = "city" value = "jalandhar"/>
          <property name = "mobile" value = "9149102604"/>
      </bean>

      <bean id = "college1" class = "com.spring.spring_concept.college" p:id = "102" p:name = "DAVIET" p:branch="BCA" />

</beans>
