<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">
       
			
	   <context:annotation-config/>
    
       <bean name="temp1" class="com.spring.annotation.Address">
		   <property name="street" value="temp street"/>
		   <property name="city" value="jalandhar"/>
	   </bean>
	   
	   
	   <bean name="temp2" class="com.spring.annotation.Address">
		   <property name="street" value="second street"/>
		   <property name="city" value="kapurthala"/>
	   </bean>
	   
	   <bean name="emp1" class="com.spring.annotation.emp"/> 

</beans>